<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="653.0" prefWidth="850.0" styleClass="root" stylesheets="@CSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Edit">
   <children>
      <Button fx:id="back" layoutX="8.0" layoutY="14.0" mnemonicParsing="false" onAction="#BackCenterAdd" prefHeight="35.0" prefWidth="35.0" stylesheets="@CSS.css">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/left-arrow-angle-in-circular-button.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <TextField fx:id="editEnglish" layoutX="282.0" layoutY="193.0" opacity="0.83" prefHeight="43.0" prefWidth="286.0" promptText="Word Target" />
      <TextField fx:id="editVienam" layoutX="280.0" layoutY="284.0" opacity="0.83" prefHeight="43.0" prefWidth="286.0" promptText="Word Explain" />
      <Button layoutX="400.0" layoutY="361.0" mnemonicParsing="false" onAction="#Edit" opacity="0.83" style="-fx-background-color: #FDFAFF;" text="Edit" />
      <MenuButton fx:id="menuButton" layoutX="2.0" layoutY="78.0" mnemonicParsing="false" style="-fx-background-color: #a5d1f8;" text="English">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#changeE" text="English" />
          <MenuItem mnemonicParsing="false" onAction="#changeV" text="Vietnamese" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
