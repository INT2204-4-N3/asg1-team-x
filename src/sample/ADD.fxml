<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="653.0" prefWidth="850.0" styleClass="root" stylesheets="@CSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller2">
   <children>
      <Button fx:id="back" layoutY="14.0" mnemonicParsing="false" onAction="#BackCenterAdd" prefHeight="35.0" prefWidth="35.0">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/left-arrow-angle-in-circular-button.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button layoutX="386.0" layoutY="354.0" mnemonicParsing="false" onAction="#ADD" opacity="0.83" style="-fx-background-color: #FDFAFF;" text="Add" />
      <TextField fx:id="addEnglish" layoutX="275.0" layoutY="162.0" opacity="0.83" prefHeight="45.0" prefWidth="287.0" promptText="Word Target" />
      <TextField fx:id="addVienam" layoutX="275.0" layoutY="251.0" opacity="0.83" prefHeight="45.0" prefWidth="287.0" promptText="Word Explain" />
      <MenuButton fx:id="menuButton" layoutY="73.0" mnemonicParsing="false" style="-fx-background-color: #a5d1f8;" text="English">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#changeE" text="English" />
          <MenuItem mnemonicParsing="false" onAction="#changeV" text="Vietnamese" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
