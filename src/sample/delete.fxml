<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="653.0" prefWidth="850.0" styleClass="root" stylesheets="@CSS.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller3">
   <children>
      <Button fx:id="back" layoutY="14.0" mnemonicParsing="false" onAction="#BackDelete" prefHeight="35.0" prefWidth="35.0" stylesheets="@CSS.css">
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/left-arrow-angle-in-circular-button.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="del" layoutX="382.0" layoutY="311.0" mnemonicParsing="false" onAction="#DELETE" opacity="0.83" prefHeight="31.0" prefWidth="86.0" style="-fx-background-color: #FDFAFF;" text="Delete" />
      <TextField fx:id="English" layoutX="285.0" layoutY="194.0" opacity="0.83" prefHeight="64.0" prefWidth="280.0" promptText="Delete">
         <font>
            <Font size="30.0" />
         </font>
      </TextField>
      <MenuButton fx:id="menuButton" layoutY="75.0" mnemonicParsing="false" style="-fx-background-color: #a5d1f8;" text="English">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#changeE" text="English" />
          <MenuItem mnemonicParsing="false" onAction="#changeV" text="Vietnamese" />
        </items>
      </MenuButton>
   </children>
</AnchorPane>
